<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "struts-2.1.dtd" >
<struts>
	<package name="qrcode" namespace="/qrcode" extends="configuration" >
		<action name="qrcode_*" class="QRcodeAction" method="{1}" >
			<result name="list">/WEB-INF/pages/mobile/qrcode/qrcode_list.jsp</result>
			<result name="edit">/WEB-INF/pages/mobile/qrcode/qrcode_edit.jsp</result>
			<result name="qrcode">/WEB-INF/pages/mobile/qrcode/qrcode_code.jsp</result>
		</action>
		
	</package>
	
</struts>
