		<action name="appCenter_*" class="appCenterHttpAction" method="{1}" >
		</action>
	</package>
	
	<package name="commonHttp" namespace="/commonHttp" extends="configuration" >
		<action name="commonHttp_*" class="commonHttpAction" method="{1}" >
		</action>
	</package>
	
	<package name="homePageHttp" namespace="/homePageHttp" extends="configuration" >
		<action name="homePageHttp_*" class="homePageHttpAction" method="{1}" >
		</action>
	</package>
	
	<package name="myPortalHttp" namespace="/myPortalHttp" extends="configuration" >
		<action name="myPortalHttp_*" class="myPortalHttpAction" method="{1}" >
		</action>
	</package>
	
	<package name="secureCenter" namespace="/secureCenter" extends="configuration" >
